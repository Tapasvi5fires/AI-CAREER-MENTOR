.vscode/
.idea/
*.sublime-workspace
*.sublime-project

# Pytest
.pytest_cache/

# Coverage
.coverage
coverage.xml
htmlcov/

# MyPy, Pyright, and other caches
.mypy_cache/
.pyre/
.pytype/
.ruff_cache/

# Jupyter Notebook checkpoints
.ipynb_checkpoints/

# Logs and local runtime files
logs/
*.log
